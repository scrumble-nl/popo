{
  "name": "scrumble-nl/popo",
  "type": "library",
  "homepage": "https://github.com/scrumble-nl/popo",
  "description": "Plain old PHP object",
  "keywords": [
    "popo",
    "php",
    "laravel",
    "dto"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Rico Clark",
      "email": "rico@scrumble.nl"
    },
    {
      "name": "Luuk de Weijer",
      "email": "luuk@scrumble.nl"
    },
    {
      "name": "Stefan Mermans",
      "email": "stefan@scrumble.nl"
    }
  ],
  "autoload": {
    "psr-4": {
      "Scrumble\\Popo\\": "src/"
    },
    "classmap": [
      "src"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "require": {
    "php": ">=8.1",
    "laravel/framework": "^9.0|^10.0|^11.0|^12.0",
    "laravel/helpers": "^1.6",
    "spatie/laravel-data": "^4.17"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.8",
    "larastan/larastan": "^2.1|^3.0",
    "orchestra/testbench": "^7.0|^8.0|^9.0|^10.0",
    "phpunit/phpunit": "^9.0|^10.0|^11.0"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Scrumble\\Popo\\PopoServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "replace": {
  },
  "scripts": {
  }
}
